<ul class="catalog">
  <% restaurants.each do |restaurant| %>
    <li>
      <%= image_tag(restaurant.image_url) %>
      <h2><%= restaurant.name %></h2>
      <h3><%= restaurant.price_type %> ></h3>
      <p>
        <%= sanitize(restaurant.description) %>
      </p>
      <p><%= restaurant.address %></p>
    </li>
  <% end %>
</ul>